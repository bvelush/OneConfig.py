{
    "OneConfig": {
        "stores": {
            "$SEC": {
                "type": "OneConfig.Stores.JsonFileStore.JsonFileStore", 
                "params": {
                    "path": "%APP_ROOT%/tests/resources/testJsonFileStore.cfg.json"
                }
            }            
        },
        "sensors": {
            "?:DEPLOY": {
                "type": "OneConfig.Sensors.EnvSensor.EnvSensor",
                "params": {
                    "envvar": "DEPLOY"
                }
            },
            "?:ENV_ABC": {
                "type": "OneConfig.Sensors.EnvSensor.EnvSensor",
                "params": {
                    "envvar": "abc"
                }
            },
            "?:DOMAIN": {
                "type": "OneConfig.Sensors.DomainSensor.DomainSensor",
                "params": {
                }
            }
        }
    }, 

    "AppName": "My Precious App: {{{AppEnv}}}",
    "AppEnv": {
        "?:ENV": {
            "DEV": "Developer edition", 
            "TEST": "UAT",
            "PROD": "Enjoy!"
        }
    }, 

    "Users": {
        "DbUser": {
            "?:ENV": {
                "DEV": "mylocaluser", 
                "TEST": "$TestVault.DbUser",
                "PROD": "$ProdVault.DbUser"
            }        
        },
        "DbUserPwd": {
            "?:ENV": {
                "DEV": "%^&xyz123", 
                "TEST": "$TestVault.DbUserPwd",
                "PROD": "$ProdVault.DbUserPwd"
            }
        }
    },

    "DB": {
        
        "server": {
            "?:ENV": {
                "DEV": "192.168.101.102", 
                "TEST": "testDbserver.mydomain.com",
                "PROD": "$ProdVault.DbServerName"
            }
        }, 
        "connString": "server={{{DB.server}}}, protocol=gRPC, transport=https, security=tls12, user={{{Users.DbUser}}}, password={{{Users.DbUserPwd}}}",
        "cacheTime": {
            "?:ENV": {
                "DEV": 60, 
                "TEST": 180,
                "PROD": 3600
            }
        }
    },

    "Modules": {
        "UI": {
            "ForceMFA": {
                "?:ENV": {
                    "DEV": false, 
                    "TEST": false,
                    "PROD": true
                }
            },
            "TokenTTL": {
                "?:ENV": {
                    "DEV": 60000, 
                    "TEST": 30000,
                    "PROD": 8
                }
            }, 
            "SearchUriPrefix": "?search&fuzzy={{{Modules.Search.AllowFuzzy}}}&regex={{{Modules.Search.AllowRegex}}}&results={{{Modules.Search.ResultsCount}}}&GoodLuck!"
        },

        "Search": {
            "AllowFuzzy": true,
            "AllowRegex": false, 
            "resultsCount": {
                "?:ENV": {
                    "DEV": 10, 
                    "TEST": 100,
                    "PROD": 200
                }
            }
        }
    }
}